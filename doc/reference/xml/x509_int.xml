<refsect2 id="schema-id" role="enum">
<title>enum schema_id</title>
<indexterm zone="schema-id"><primary>schema_id</primary></indexterm>
<programlisting>typedef enum {
  PBES2_GENERIC,                /* when the algorithm is unknown, temporal use when reading only */
  PBES2_3DES,                   /* the stuff in PKCS #5 */
  PBES2_AES_128,
  PBES2_AES_192,
  PBES2_AES_256,
  PKCS12_3DES_SHA1,             /* the stuff in PKCS #12 */
  PKCS12_ARCFOUR_SHA1,
  PKCS12_RC2_40_SHA1
} schema_id;
</programlisting>
<para>
</para></refsect2>

